<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.braintraining_mindmemory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_puzzle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_puzzle_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GraphActivity"></activity>
        <activity android:name=".StatusActivity" />
        <activity android:name=".QuizStartActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".WelcomeToActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>